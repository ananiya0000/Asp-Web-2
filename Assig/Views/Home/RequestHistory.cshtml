@model IEnumerable<Assig.Models.Notification>

@{
    ViewBag.Title = "RequestHistory";
}

<h2>RequestHistory</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OwnerEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MachineID)
        </th>
        <th>
            Catagory
        </th>
        <th>
            Industry
        </th>
        <th>
            Machine Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Seen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestDate)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.CustomerEmail)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OwnerEmail)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MachineID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Machine.Catagory)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Machine.Industry)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Machine.MachineName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Seen)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RequestDate)
    </td>
</tr>
}

</table>
